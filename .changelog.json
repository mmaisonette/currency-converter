{
    "template": "## {{release}}\n\n{{groups}}",
    "groups": [
      {
        "title": "🚀 Features",
        "regexp": "^feat",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 1
      },
      {
        "title": "🐛 Bug Fixes",
        "regexp": "^fix",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 2
      },
      {
        "title": "📝 Documentation",
        "regexp": "^docs",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 3
      },
      {
        "title": "♻️ Refactoring",
        "regexp": "^refactor",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 4
      },
      {
        "title": "🧪 Tests",
        "regexp": "^test",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 5
      },
      {
        "title": "🔧 Chores",
        "regexp": "^chore",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 6
      },
      {
        "title": "Other Changes",
        "regexp": ".*",
        "commits-format": "- `{{hash:7}}` {{subject}}",
        "order": 7
      }
    ],
    "output": "CHANGELOG.md",
    "breaking-pattern": "BREAKING CHANGE:"
  }